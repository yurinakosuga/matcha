<h2>店情報一覧・追加</h2>

  <%= form_with model: @shop, url:admin_shops_path do |f| %>
    <h4>店名</h4>     
    <%= f.text_field :name %>
     <%= f.submit '新規登録', class:"btn btn-success" %>
  <% end %>
  
  <h4>店情報</h4>
  <% @shops.each do |shop| %>

  
  <span><%= shop.name %></span>
  <%= link_to "編集する", edit_admin_shop_path(genre.id), class:"btn btn-success" %>
  
<% end %>

